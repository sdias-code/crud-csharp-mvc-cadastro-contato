@model ContatoModel
@{
    ViewData["Title"] = "Apagar cadastro";
    }

<div class="text-center">
    <h3>Deseja realmente a pagar o cadastro de @Model.Nome ?</h3>


</div>
<a role="button" class="btn btn-secondary btn-block" asp-controller="Contato" asp-action="Index">Cancelar</a>
<a role="button" class="btn btn-danger btn-block" asp-controller="Contato" asp-action="Apagar" asp-route-id="@Model.Id">Confirmar Exclusão</a>